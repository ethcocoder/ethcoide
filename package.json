{
  "name": "ai-powered-ide",
  "version": "1.0.0",
  "description": "AI-powered desktop IDE with Gemini integration",
  "main": "dist/main/main.js",
  "scripts": {
    "build": "tsc",
    "build:watch": "tsc --watch",
    "start": "npm run build && electron .",
    "dev": "concurrently \"npm run build:watch\" \"wait-on dist/main/main.js && electron .\"",
    "test": "jest",
    "test:watch": "jest --watch"
  },
  "keywords": [
    "ide",
    "ai",
    "electron",
    "monaco",
    "gemini"
  ],
  "author": "Ethco Coder",
  "license": "MIT",
  "devDependencies": {
    "@types/jest": "^29.0.0",
    "@types/node": "^20.0.0",
    "concurrently": "^8.0.0",
    "electron": "^28.0.0",
    "fast-check": "^4.5.3",
    "jest": "^29.0.0",
    "jest-environment-jsdom": "^30.2.0",
    "jsdom": "^27.4.0",
    "ts-jest": "^29.0.0",
    "typescript": "^5.0.0",
    "wait-on": "^7.0.0"
  },
  "dependencies": {
    "chokidar": "^3.5.0",
    "keytar": "^7.9.0",
    "monaco-editor": "^0.55.1"
  }
}
